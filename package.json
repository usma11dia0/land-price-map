{
  "name": "land-price-map",
  "version": "1.0.0",
  "type": "module",
  "description": "地価情報マップ - 日本の地価情報を地図上で確認できるWebアプリケーション",
  "main": "dist/app.js",
  "scripts": {
    "prebuild": "node -e \"import('fs').then(fs => { if(!fs.existsSync('src/config.ts')){fs.copyFileSync('src/config.example.ts','src/config.ts');} })\"",
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "npx http-server -p 8080 -o",
    "start": "npm run build && npm run serve",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:headed": "npx playwright test --headed"
  },
  "keywords": [
    "land-price",
    "map",
    "leaflet",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/leaflet": "^1.9.8",
    "@vercel/node": "^3.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2"
  }
}
